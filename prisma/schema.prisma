generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product {
  id    String @id
  name  String
  price Float
}

model Customer {
  id           String  @id
  name         String
  street       String?
  number       Int?
  zipcode      String?
  city         String?
  active       Boolean
  rewardPoints Int
}
